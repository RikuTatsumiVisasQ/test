{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "packageRules": [
        {
            "enabled": false,
            "matchPackagePatterns": [
                "*"
            ]
        }
    ],
    "vulnerabilityAlerts": {
        "enabled": true,
        "addLabels": [
            "security"
        ]
    }
}